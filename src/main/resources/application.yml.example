spring:
  jpa:
    #To display SQL statements generated by JPA, uncomment this line:
    #show-sql: true
    hibernate:
      ddl-auto: update
  data:
    rest:
      base-path: /api
  ssl:
    bundle:
      pem:
        es-client:
          truststore:
          # Put the CA that signed the ES cert here.
            certificate: "classpath:dev-only-ca.crt"
es:
  host: <Elastic search IP/Hostname>
  port: 9200
# Set this to the CN of the Elastic Search's certificate when you connect to IP directly.
#  sslEnforceCN: leak-db-es-http.leak-db.es.local
  username: <ES Username>
  password: <ES password>
springdoc:
  api-docs:
    path: /api-docs
server:
  port: 8080
